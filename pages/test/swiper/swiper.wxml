<!--是否显示圆点，自动播放， 间隔时间， 监听滚动和点击事件-->
<swiper indicator-dots="true" autoplay="true" duration="1000" bindchange="listenSwiper" >
<!--swiper-item只能包含一个节点再多会自动删除-->
    <swiper-item>
        <view style="background: #dddddd; height: 150px"></view>
    </swiper-item>
    <swiper-item>
        <view style="background: #ffaaaa; height: 150px"></view>
    </swiper-item>
    <swiper-item>
        <view style="background: #12f231; height: 150px"></view>
    </swiper-item>
    <swiper-item>
        <view style="background: #768dec; height: 150px"></view>
    </swiper-item>     
</swiper>
<view>
    <text>{{text}}</text>
</view>